def TheRestOfTheDays5():
  print('yo')

    

def ScheduleOperator():
  print("Monday's schedule?")
  if input() == 'yes':
    print('ok')
    Monday()
  else:
    print('ok')

  print('would you like to see more schedules?')
  if input() == 'yes':
    TheRestOfTheDays()
  else:
    print('ok')
 
   

  print('would you like to see more schedules?')
  if input() == 'yes':
    TheRestOfTheDays2()
  else:
   print('ok')

  print('would you like to see more schedules?')
  if input() == 'yes':
    print('ok')
    TheRestOfTheDays3()
  


    

