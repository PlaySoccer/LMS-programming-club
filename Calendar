def TheRestOfTheDays():
  print("would you like to see Tuesday's schedule?")
  if input() == 'yes':
    print('ok')
    Tuesday()
  else:
    print('ok')

def TheRestOfTheDays2():
  print("would you like to see Wednesday's schedule")
  if input() == 'yes':
    Wednesday()

def TheRestOfTheDays3():
